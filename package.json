{
	"name": "vue-agile",
	"version": "1.1.0",
	"description": "A carousel component for Vue.js",
	"author": "≈Åukasz Florczak <lukasz@florczak.dev>",
	"scripts": {
		"serve": "vue-cli-service serve --open ./demo/main.js",
		"test:unit": "vue-cli-service test:unit",
		"lint": "vue-cli-service lint",
		"build-bundle": "vue-cli-service build --target lib --name VueAgile ./src/Agile.vue && rm dist/demo.html",
		"build-docs": "vue-cli-service build ./demo/main.js --dest ./docs --modern"
	},
	"main": "src/index.js",
	"unpkg": "dist/VueAgile.umd.min.js",
	"files": [
		"dist/*",
		"src/*",
		"*.json",
		"*.js"
	],
	"dependencies": {
		"lodash.orderby": "^4.6.0",
		"lodash.throttle": "^4.1.1"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "~4.3.0",
		"@vue/cli-plugin-eslint": "~4.3.0",
		"@vue/cli-plugin-unit-jest": "^4.3.1",
		"@vue/cli-service": "^4.3.1",
		"@vue/compiler-sfc": "^3.0.0-beta.1",
		"@vue/eslint-config-standard": "^5.1.2",
		"@vue/test-utils": "^2.0.0-alpha.1",
		"babel-eslint": "^10.1.0",
		"eslint": "^6.7.2",
		"eslint-plugin-pug": "^1.2.2",
		"eslint-plugin-vue": "^7.0.0-alpha.0",
		"pug": "^2.0.3",
		"pug-plain-loader": "^1.0.0",
		"sass": "^1.26.3",
		"sass-loader": "^8.0.2",
		"vue": "^3.0.0-alpha.4",
		"vue-cli-plugin-pug": "~1.0.7",
		"vue-cli-plugin-vue-next": "~0.1.2",
		"vue-template-compiler": "^2.5.21"
	},
	"bugs": {
		"url": "https://github.com/lukaszflorczak/vue-agile/issues"
	},
	"keywords": [
		"vue",
		"component",
		"slider",
		"carousel",
		"slide"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lukaszflorczak/vue-agile.git"
	}
}
